{
  "name": "fastify-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "set NODE_ENV=development&&node index.js | pino-pretty",
    "log": "node print-log.ts",
    "format": "prettier ./src index.js global-constants.js --write",
    "lint": "eslint ./src index.js global-constants.js --fix",
    "start:dev": "set NODE_ENV=development&&nodemon index.js | pino-pretty",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky && husky install",
    "lint-staged": "lint-staged --concurrent false",
    "migrate:make": "set NODE_ENV=development&&knex migrate:make",
    "migrate:latest": "set NODE_ENV=development&&knex migrate:latest",
    "migrate:list": "set NODE_ENV=development&&knex migrate:list",
    "migrate:rollback": "set NODE_ENV=development&&knex migrate:rollback",
    "migrate:down": "set NODE_ENV=development&&knex migrate:down",
    "seed:make": "set NODE_ENV=development&&knex seed:make",
    "seed:run": "set NODE_ENV=development&&knex seed:run"
  },
  "imports": {
    "#repository/*": "./src/repository/*",
    "#utilities/*": "./src/utilities/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.592.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "github:AsadGG/fastify-swagger-ui",
    "@sendgrid/mail": "^8.1.3",
    "@sinclair/typebox": "^0.32.10",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.25.2",
    "fastify-bcrypt": "^1.0.1",
    "fastify-plugin": "^4.5.1",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "puppeteer": "^22.12.1",
    "sonic-boom": "^3.8.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@inquirer/prompts": "^3.3.0",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "glob": "^10.3.10",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.4",
    "pretty-bytes": "^6.1.1"
  }
}
